{
    "name": "spatie/laravel-mailcoach-sendgrid-setup",
    "description": "Setup Sendgrid for use with Mailcoach",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-mailcoach-sendgrid-setup"
    ],
    "homepage": "https://github.com/spatie/laravel-mailcoach-sendgrid-setup",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.222",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachSendgridSetup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachSendgridSetup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Spatie\\MailcoachSendgridSetup\\MailcoachSendgridSetupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
