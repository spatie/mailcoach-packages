{
    "name": "spatie/laravel-mailcoach-sendinblue-feedback",
    "description": "Process feedback for email campaigns sent using Sendinblue",
    "keywords": [
        "spatie",
        "laravel-mailcoach-sendinblue-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Rias Van der Veken",
            "email": "rias@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
    {
        "type": "composer",
        "url": "https://satis.spatie.be"
    }
],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "symfony/sendinblue-mailer": "^6.0",
        "spatie/laravel-mailcoach": "^7.0"
    },
    "require-dev": {
        "ext-json": "*",
        "fakerphp/faker": "^1.12",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachSendinblueFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachSendinblueFeedback\\Tests\\": "tests",
            "Spatie\\Mailcoach\\Database\\Factories\\": "vendor/spatie/laravel-mailcoach/database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachSendinblueFeedback\\MailcoachSendinblueFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
